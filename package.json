{
  "name": "tiktok-chat-reader",
  "version": "3.0.0",
  "description": "TikTok Live Chat Reader with dual connection support (TikTok-Live-Connector + Euler WebSocket API) built with Bun + Hono + TypeScript",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "test": "bun test",
    "check": "bunx @biomejs/biome check .",
    "fix": "bunx @biomejs/biome check --write ."
  },
  "dependencies": {
    "hono": "^4.12.2",
    "tiktok-live-connector": "^2.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
